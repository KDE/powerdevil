<?xml version="1.0"?>
<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.PowerManagement.ScreenBrightnessControl.Display">
    <property name="Label" type="s" access="read"/>
    <property name="Brightness" type="i" access="read"/>
    <property name="MaxBrightness" type="i" access="read"/>
    <property name="MinBrightness" type="i" access="read"/>
    <property name="KnownSafeMinBrightness" type="i" access="read"/>
    <property name="PreferredNumberOfBrightnessSteps" type="i" access="read"/>
    <method name="SetBrightness">
      <arg name="brightness" type="i" direction="in"/>
      <arg name="source_client_context" type="i" direction="in"/>
      <arg name="flags" type="u" direction="in"/>
    </method>
    <signal name="BrightnessChanged">
      <arg name="brightness" type="i" direction="out"/>
      <arg name="source_client_name" type="i" direction="out"/>
      <arg name="source_client_context" type="i" direction="out"/>
    </signal>
    <signal name="MaxBrightnessChanged">
      <arg type="i" direction="out"/>
    </signal>
    <signal name="MinBrightnessChanged">
      <arg type="i" direction="out"/>
    </signal>
  </interface>
</node>
