#: activitywidget.cpp:104
#, kde-format
msgid "Shut down"
msgstr "シャットダウン"

#: activitywidget.cpp:108
#, kde-format
msgid "PC running on AC power"
msgstr "AC アダプタで動作"

#: activitywidget.cpp:109
#, kde-format
msgid "PC running on battery power"
