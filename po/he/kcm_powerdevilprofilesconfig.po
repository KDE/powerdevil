msgstr[1] "אחרי שתי דקות"
msgstr[2] "אחרי %1 דקות"
msgstr[3] "אחרי %1 דקות"

#: ui/TimeDelaySpinBox.qml:28
#, kde-format
msgid "after %1 sec"
msgid_plural "after %1 sec"
msgstr[0] "אחרי שנייה"
msgstr[1] "אחרי שתי שניות"
